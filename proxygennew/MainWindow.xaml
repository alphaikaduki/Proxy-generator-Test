<Window x:Class="proxygennew.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proxygennew"
        mc:Ignorable="d"
        Title="Alpha-ProxyGen" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="98*"/>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="685*"/>
        </Grid.ColumnDefinitions>
        <CheckBox x:Name="http" Content="http/s" Grid.Column="2" HorizontalAlignment="Left" Margin="537,113,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="socks5" Content="socks5" Grid.Column="2" HorizontalAlignment="Left" Margin="537,133,0,0" VerticalAlignment="Top"/>
        <Button x:Name="start" Content="Start" Grid.Column="2" HorizontalAlignment="Left" Margin="482,336,0,0" VerticalAlignment="Top" Width="172" Height="46" Click="start_Click"/>
        <TextBox x:Name="proxylist" Grid.Column="2" HorizontalAlignment="Left" Height="295" Margin="10,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="410"/>

    </Grid>
</Window>
